<?xml version="1.0" encoding="utf-8"?>

<MessagingConfiguration 
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="MessagingConfig.xsd">

  <Common>
    <Configuration>
      <!-- <Item Name="ForceSerializationStyle">NetDataContractSerializer</Item> -->
      <Item Name="EncryptedSession">true</Item>
    </Configuration>
  </Common>
  
  <MessageBus Name="RestfulAgentToGatewayBus" Enabled="true" >
    <Dependencies>
			<Assembly>Eze.NetEx.dll</Assembly>
      <Assembly>Eze.Ims.Platform.Communication.Wcf.dll</Assembly>
    </Dependencies>
    <Configuration>
      <Item Name="MaxPollInterval">00:00:03.000</Item>
      <Item Name="MinPollInterval">00:00:00:500</Item>
      <Item Name="EncryptionEnabled">true</Item>
      <Item Name="SubscriptionRequestBatching">true</Item>
    </Configuration>
  </MessageBus>

  <MessageBus Name="RtiDdsAgentToGatewayBus" Enabled="false" >
    <Dependencies>
      <Assembly>Eze.NetEx.dll</Assembly>
      <Assembly>Eze.Ims.Platform.Communication.RtiDds.dll</Assembly>
    </Dependencies>
    <Configuration>
      <Item Name="QosProfileFileName">USER_QOS_PROFILES.xml</Item>
      <Item Name="QosLibraryName">TCP_Unicast</Item>
      <Item Name="QosProfileName">AgentDomainParticipant</Item>
    </Configuration>
  </MessageBus>

</MessagingConfiguration>
