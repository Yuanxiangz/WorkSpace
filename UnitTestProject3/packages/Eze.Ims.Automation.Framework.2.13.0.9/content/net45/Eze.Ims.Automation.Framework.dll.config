<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="specFlow" type="TechTalk.SpecFlow.Configuration.ConfigurationSectionHandler, TechTalk.SpecFlow" />
  </configSections>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
  </configSections>
  <appSettings>
    <add key="EmailWebServer" value="https://mail.objectivasoftware.com.cn/EWS/Exchange.asmx" />
    <add key="EmailUser" value="ezeautoqatest" />
    <add key="EmailDomain" value="beijing" />
    <add key="EmailPassword" value="e.o7a@bj" />
    <!--These mail settings are used to parse temporary password for new user-->
    <add key="EmailAddress" value="EzeAutoqaTest@Objectivasoftware.com" />
    <!--New created user email address-->
    <add key="NewUserPassword" value="123456" />
    <add key="GatewayURI" value="http://localhost:4000/Gateway" />
    <add key="WebApiPort" value="9001"/>
    <add key="IsFakeDperm" value="false" />
    <add key="IsEmailReachable" value="true" />
    <!--test will not change user password when using fake Dperm-->
    <add key="GlobalDataInitialized" value="false" />
    <!--check if we have to initialize global data-->
    <add key="ServiceDefaultPath" value="D:\IMS" />
    <add key="HostFirm" value="Eze" />
    <add key="HostFirmUser" value="eze1" />
    <add key="HostFirmPwd" value="eze1" />
    <add key="HostFirmUser2" value="eze2" />
    <add key="HostFirmPwd2" value="eze2" />
    <add key="ClientSettingsProvider.ServiceUri" value="" />
    <add key="DPermsLatencySecs" value ="5"/>
  </appSettings>
  <specFlow>
    <!-- For additional details on SpecFlow configuration options see http://go.specflow.org/doc-config -->
  </specFlow>
  <log4net>
    <appender name="TestLogFileAppender" type="log4net.Appender.RollingFileAppender">
      <file type="log4net.Util.PatternString" value="Log\ImsTestLog_.log" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern type="log4net.Util.PatternString" value="Level=&quot;%%-5level&quot;, Time=&quot;%%date&quot;, Class=&quot;%%logger&quot;, Message=&quot;%%message&quot;, Exception=&quot;%%exception&quot;%newline" />
      </layout>
      <appendToFile value="true" />
      <rollingStyle value="Composite" />
      <maxSizeRollBackups value="5" />
      <maximumFileSize value="5MB" />
      <staticLogFileName value="false" />
      <datePattern value="yyyyMMdd" />
      <preserveLogFileNameExtension value="true" />
    </appender>
    <appender name="ConsoleAppender" type="log4net.Appender.ConsoleAppender">
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %-5level %logger [%property{NDC}] - %message%newline" />
      </layout>
    </appender>
    <root>
      <appender-ref ref="TestLogFileAppender" />
      <appender-ref ref="ConsoleAppender" />
    </root>
  </log4net>
  <system.web>
    <membership defaultProvider="ClientAuthenticationMembershipProvider">
      <providers>
        <add name="ClientAuthenticationMembershipProvider" type="System.Web.ClientServices.Providers.ClientFormsAuthenticationMembershipProvider, System.Web.Extensions, Version=4.0.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" serviceUri="" />
      </providers>
    </membership>
    <roleManager defaultProvider="ClientRoleProvider" enabled="true">
      <providers>
        <add name="ClientRoleProvider" type="System.Web.ClientServices.Providers.ClientRoleProvider, System.Web.Extensions, Version=4.0.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" serviceUri="" cacheTimeout="86400" />
      </providers>
    </roleManager>
  </system.web>
</configuration>